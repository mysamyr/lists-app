{
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"rules": {
		"no-console": "warn",
		"no-undef": "error",
		"semi": "error",
		"semi-spacing": "error",
		"eqeqeq": "warn",
		"object-shorthand": "error",
		"no-unused-vars": [
			"error",
			{
				"argsIgnorePattern": "next"
			}
		]
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	}
}
